#include <iostream>
#include <cstdio>
using namespace std;


     
int maximum(int a, int b, int c, int d)
{ int max;
   

if(a>b&& a>c&& a>d)
    {
      
        max=a;

    }
    else if(b>c&&b>d )
    {
       
        max=b;
    }
    else if(c>d)
    {
       
        max=c;

    }
    else
     {

    max=d;

    }
 return max;
}
int main()
{   
    int a,b,c,d;
    cin>>a>>b>>c>>d;
int max=maximum(a,b,c,d);
    cout<<max;
    
    
    return 0;
}
